    "def numero_mas_repetido(lista):\n",
    "    contador = {}\n",
    "    for num in lista:\n",
    "        if num in contador.keys():\n",
    "            contador[num] += 1\n",
    "        else:\n",
    "            contador[num] = 1\n",
    "\n",
    "    maximo_repeticion = 0\n",
    "    mas_repetido = None\n",
    "\n",
    "    for numero, repeticion  in contador.items():\n",
    "        if repeticion > maximo_repeticion:\n",
    "            maximo_repeticion = repeticion\n",
    "            mas_repetido = numero\n",
    "\n",
    "\n",
    "    return mas_repetido, maximo_repeticion"
   ]
